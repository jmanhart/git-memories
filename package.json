{
  "name": "git-memories",
  "version": "1.0.3",
  "description": "A fun CLI tool to see your GitHub contributions on this day throughout the years",
  "main": "dist/index.js",
  "bin": {
    "git-memories": "dist/index.js"
  },
  "scripts": {
    "build": "npx tsc --module CommonJS --target ES2022 --esModuleInterop --outDir dist src/index.ts",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "git",
    "github",
    "cli",
    "contributions",
    "memories",
    "developer-tools",
    "time-travel",
    "history",
    "nostalgia"
  ],
  "author": "John Manhart",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmanhart/git-memories"
  },
  "homepage": "https://github.com/jmanhart/git-memories",
  "bugs": {
    "url": "https://github.com/jmanhart/git-memories"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "open": "^7.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.9",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
